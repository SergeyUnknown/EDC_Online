<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<location path="Audits.aspx">
		<system.web>
			<authorization>
				<deny roles="Monitor"/>
			</authorization>
		</system.web>
	</location>
	<system.web>
		<authorization>
			<deny users="?"/>
			<allow roles="Administrator, Data_Manager, Monitor"/>
			<deny users="*"/>
		</authorization>
	</system.web>
</configuration>